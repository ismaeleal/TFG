      event.initEvent('recommendationsRendered', true, true);
      document.dispatchEvent(event);
    }

    
      var sidebarModule = document.getElementsByClassName('related-alts');
      if (sidebarModule.length > 0) {
        sidebarModule[0].innerHTML += "\n <p class=\"aside-title\">TE RECOMENDAMOS</p>\n <ul>\n  \n   \n   <li class=\"related-item\" data-container=\"v8\" >\n    <a href=\"/ordenadores/la-busqueda-del-zen-en-el-escritorio-sin-cables-el-orden-frente-al-caos\">\n     <img\n      src=\"https://i.blogs.es/ed686d/desk/150_150.jpg\"\n      alt=\"La b√∫squeda del zen en el escritorio sin cables: el orden frente al caos\"\n     >\n     <div class=\"related-item-info\">\n      <h3 class=\"related-item-title\">La b√∫squeda del zen en el escritorio sin cables: el orden frente al caos</h3>\n     </div>\n    </a>\n   </li>\n  \n   \n   <li class=\"related-item\" data-container=\"v8\" >\n    <a href=\"https://www.xatakahome.com/trucos-y-bricolaje-smart/como-acabar-de-una-vez-por-todas-con-los-cables-en-el-escritorio-i\">\n     <img\n      src=\"https://i.blogs.es/91ee28/0403586_pe565641_s5/150_150.jpg\"\n      alt=\"C√≥mo acabar de una vez por todas con los cables en el escritorio (I)\"\n     >\n     <div class=\"related-item-info\">\n      <h3 class=\"related-item-title\">C√≥mo acabar de una vez por todas con los cables en el escritorio (I)</h3>\n     </div>\n    </a>\n   </li>\n  \n   \n   <li class=\"related-item\" data-container=\"v4\" >\n    <a href=\"https://www.decoesfera.com/varios/once-ambientes-con-ch24-que-hans-jorgensen-wegner-visiono-desde-los-anos-cincuenta\">\n     <img\n      src=\"https://i.blogs.es/8392fb/ch24-wishbone-chair-colors-hans-wegner-carl-hansen-and-son-6/150_150.jpg\"\n      alt=\"Once ambientes con CH24 que Hans Jorgensen Wegner vision√≥ desde los a√±os cincuenta\"\n     >\n     <div class=\"related-item-info\">\n      <h3 class=\"related-item-title\">Once ambientes con CH24 que Hans Jorgensen Wegner vision√≥ desde los a√±os cincuenta</h3>\n     </div>\n    </a>\n   </li>\n  \n </ul>\n";
        event.initEvent('sidebarRecommendationsRendered', true, true);
        document.dispatchEvent(event);
      }
    
    customTracker.init();
  }

  if ("loading" !== document.readyState) {
    renderRecommendations();
  } else {
    document.addEventListener("DOMContentLoaded", renderRecommendations);
  }
})();
